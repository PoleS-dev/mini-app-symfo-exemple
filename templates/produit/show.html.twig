{% extends 'base.html.twig' %}



{% block body %}
    <h1>Produit {{ produit.id }}</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prix</th>
                <th>Stock</th>
             
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>{{ produit.id }}</td>
                <td>{{ produit.nom }}</td>
                <td>{{ produit.prix }}</td>
                <td>{{ produit.stock }}</td>
      
            </tr>
        </tbody>
    </table>
{% endblock %}
